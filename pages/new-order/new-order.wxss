.container {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #e8f4e8;
}

.message {
  font-size: 36rpx;
  color: #2b7d81;
  margin-bottom: 40rpx;
}

.back-button {
  width: 200rpx;
  height: 70rpx;
  background-color: #2b7d81;
  color: white;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 6rpx;
} 